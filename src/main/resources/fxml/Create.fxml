<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox minHeight="320" minWidth="320" prefHeight="315.0" prefWidth="309.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.FXMLCreateController">
   <children>
      <GridPane alignment="CENTER" hgap="20.0" nodeOrientation="LEFT_TO_RIGHT" vgap="20.0">
        <columnConstraints>
          <ColumnConstraints />
          <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" />
          <RowConstraints minHeight="10.0" />
        </rowConstraints>
         <children>
            <Label fx:id="file_name" text="File name" />
            <TextField GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" />
            <Label alignment="TOP_LEFT" text="Tags" GridPane.rowIndex="1" GridPane.valignment="TOP" />
            <VBox fx:id="vboxTags" spacing="5.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <TextField fx:id="tag1" alignment="TOP_LEFT" />
                  <TextField fx:id="tag2" alignment="TOP_LEFT" />
                  <TextField fx:id="tag3" alignment="TOP_LEFT" />
                  <TextField fx:id="tag4" alignment="TOP_LEFT" />
               </children>
               <padding>
                  <Insets top="5.0" />
               </padding>
            </VBox>
         </children>
      </GridPane>
      <Region VBox.vgrow="ALWAYS" />
      <HBox alignment="CENTER" spacing="20.0">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <children>
            <Button fx:id="create" alignment="TOP_RIGHT" mnemonicParsing="false" onAction="#create" text="Create" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
